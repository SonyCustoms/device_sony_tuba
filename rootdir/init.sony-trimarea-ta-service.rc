#
# init.sony-trimarea-ta-ccci-service.rc
#

on post-fs-data
    start ta_ccci_service

# Trim Area CCCI service
service ta_ccci_service /system/bin/ta_ccci_service
    user root
    group root radio
    seclabel u:r:ta_ccci_service:s0
    disabled
